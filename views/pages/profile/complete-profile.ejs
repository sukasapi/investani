	<script>
		$('#datepicker').datepicker({
			uiLibrary: 'bootstrap4'
		});
		$('#pic-datepicker').datepicker({
			uiLibrary: 'bootstrap4'
		});

		$('#profile-form').on('submit', function (event) {
			$('#profile-button').attr('disabled', 'disabled');
			$('#profile-spinner').show();
		});

		$('#occupation-form').on('submit', function (event) {
			$('#occupation-button').attr('disabled', 'disabled');
			$('#occupation-spinner').show();
		});

		$('#document-form').on('submit', function (event) {
			$('#document-button').attr('disabled', 'disabled');
			$('#document-spinner').show();
		});

		$('#bank-form').on('submit', function (event) {
			$('#bank-button').attr('disabled', 'disabled');
			$('#bank-spinner').show();
		});

		$(document).ready(function () {
			var registration = '<%= registration_type %>';
			document.getElementById('identity-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});
			document.getElementById('npwp-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});
			document.getElementById('account-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});
			document.getElementById('pic-identity-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});
			document.getElementById('company-registration-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});
			document.getElementById('sk-kemenkumham-number').addEventListener('keydown', function(e) {
				if (e.which === 38 || e.which === 40) {
					e.preventDefault();
				}
			});

			if (<%= profile_length %> == 0) {
				$('#profile-form').show()
			}
			if (<%= profile_length%> != 0 && registration == 'individual') {
				if (<%= occupation_length %> == 0) {
					$('#header-form').text('Pekerjaan');
					$('#occupation-form').show();

					$('.first-list').click(function () {
						$('.first-list').attr('class', 'first done first-list');
						$('.second-list').attr('class', 'done second-list');

						$('#header-form').text('Profil');
						$('#profile-form').show();
						$('#occupation-form').hide();
						$('#document-form').hide();

						$('#name').val('<%= name %>');
						$('#phone').val('<%= phone %>');
						$('#datepicker').val('<%= birth_date %>');
						$('#address').val('<%= address %>');
						$('#registration_type').val(registration);
						$('#name').attr('placeholder', 'Nama Lengkap');
						$('#phone-input').attr('class', 'form-holder');
						$('#phone').attr('placeholder', 'Nomor Handphone');
						$('#gender-input').show();
						if ('<%= gender %>' == 'male') {
							$('#gender-male').attr('selected', 'selected');
						}
						else {
							$('#gender-female').attr('selected', 'selected');
						}
						$('#established-place-company-phone-row').hide();
						$('#datepicker').attr('placeholder', 'Tanggal Lahir');
						$.when(
							$.ajax({
								type: 'get',
								url: '/complete-profile/get-city',
								data: {
									'province_id': <%= province_id %>
								},
								success: function(data, status) {
									if(data.success == true) {     
										for (let index = 0; index < data.city.length; index++) {
											if (data.city[index].id == <%= city_id %>) {
												$('#city').append('<option class="city-option" value="'+data.city[index].id+'" selected>'+data.city[index].nama+'</option>');
											}
											else {
												$('#city').append('<option class="city-option" value="'+data.city[index].id+'">'+data.city[index].nama+'</option>');
											}
										}
									}
								}
							}),
		
							$.ajax({
								type: "GET",
								url: '/complete-profile/get-district',
								data: {
									'city_id': <%= city_id %>
								},
								success: function(data, status) {
									if(data.success == true) {
										for (let index = 0; index < data.district.length; index++) {
											if (data.district[index].id == <%= district_id %>) {
												$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'" selected>'+data.district[index].nama+'</option>');
											}
											else {
												$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'">'+data.district[index].nama+'</option>');
											}
										}
									}
								}
							}),
							$.ajax({
								type: "GET",
								url: '/complete-profile/get-sub_district',
								data: {
									'district_id': <%= district_id %>
								},
								success: function(data, status) {
									if(data.success == true) {
