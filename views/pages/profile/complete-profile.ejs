									'city_id': <%= city_id %>
								},
								success: function(data, status) {
									if(data.success == true) {
										for (let index = 0; index < data.district.length; index++) {
											if (data.district[index].id == <%= district_id %>) {
												$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'" selected>'+data.district[index].nama+'</option>');
											}
											else {
												$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'">'+data.district[index].nama+'</option>');
											}
										}
									}
								}
							}),
							$.ajax({
								type: "GET",
								url: '/complete-profile/get-sub_district',
								data: {
									'district_id': <%= district_id %>
								},
								success: function(data, status) {
									if(data.success == true) {
										for (let index = 0; index < data.sub_district.length; index++) {
											if (data.sub_district[index].id == <%= sub_district_id %>) {
												$('#sub-district').append('<option class="sub_ditrict-option" value="'+data.sub_district[index].id+'" selected>'+data.sub_district[index].nama+'</option>');
											}
											else {
												$('#sub-district').append('<option class="sub_ditrict-option" value="'+data.sub_district[index].id+'">'+data.sub_district[index].nama+'</option>');
											}
										}
									}
								}
							})
						).then(function() {
						});
					});
					$('.second-list').click(function () {
						$('.first-list').attr('class', 'first done first-list');
						$('.second-list').attr('class', 'done second-list');
						$('.third-list').attr('class', 'done third-list');
						$('.last-list').attr('class', 'last done last-list');
						
						$('#header-form').text('Penanggungjawab');
						$('#pic-form').show();
						$('#profile-form').hide();
						$('#document-form').hide();
						$('#bank-form').hide();

						$('#pic-name').val('<%= name %>');
						$('#pic-datepicker').val('<%= pic_birth_date %>');
						$('#pic-identity-number').val('<%= pic_identity_number %>');
						$('#pic-identity-image-label').show();
						$('#pic-identity-image-label').text('<%= pic_identity_image %>');
						$('#pic-identity-selfie-image-label').show();
						$('#pic-identity-selfie-image-label').text('<%= pic_identity_selfie_image %>');
					});
					$('.third-list').click(function () {
						$('.first-list').attr('class', 'first done first-list');
						$('.second-list').attr('class', 'done second-list');
						$('.third-list').attr('class', 'current third-list');
						$('.last-list').attr('class', 'last done last-list');

						$('#header-form').text('Dokumen');
						$('#document-form').show();
						$('#pic-form').hide();
						$('#profile-form').hide();
						$('#bank-form').hide();

						$('#company-registration-number-input').show();
						$('#company-registration-image-input').show();
						$('#sk-kemenkumham-number-row').show();
						$('#sk-kemenkumham-image-row').show();
						$('#business-permit-image-row').show();
						$('#npwp-number-label').text('Nomor NPWP Perusahaan');
						$('#npwp-image-label').text('Unggah NPWP Perusahaan');
						$('#identity-number-input').hide();
						$('#identity-image-input').hide();
						$('#identity-selfie-image-row').hide();

						$('#company-registration-number').val('<%= company_registration_number %>');
						$('#company-registration-label').val('<%= company_registration_image %>');
						$('#npwp-number').val('<%= npwp_number %>');
						$('#npwp-image-label').val('<%= npwp_image %>');
						$('#sk-kemenkumham-number').val('<%= sk_kemenkumham_number %>');
						$('#sk-kemenkumham-image-label').val('<%= sk_kemenkumham_image %>');
						$('#business-permit-image-label').val('<%= business_permit_image %>');
					});
					$('.last-list').click(function () {
						$('#header-form').text('Bank');
						$('#bank-form').show();
						$('#profile-form').hide();
						$('#pic-form').hide();
						$('#document-form').hide();
					});
				}
			}
		});
	</script>

	<script>
		$('#registration_type').on('change', function () {
			event.preventDefault();
			var registration_type = $('#registration_type option:selected').val();
			var province_name = $('#province option:selected').text();
			var city_name = $('#city option:selected').text();
			var district_name = $('#district option:selected').text();
			var sub_district_name = $('#sub-district option:selected').text();
			$('#name').val('');
			$('#phone').val('');
			$('#established-place').val('');
			$('#company-phone').val('');
			$('#datepicker').val('');


			if (registration_type == 'individual') {
				$('#name-label').text('Nama Lengkap');
				$('#name').attr('placeholder', 'Nama Lengkap');
				$('#phone-input').attr('class', 'form-holder');
				$('#phone-label').text('Nomor Handphone');
				$('#phone').attr('placeholder', 'Nomor Handphone');
				$('#gender-input').show();
				$('#established-place-company-phone-row').hide();
				$('#established-place').val('');
				$('#company-phone').val('');
				$('#datepicker').attr('placeholder', 'Tanggal Lahir');
				$('#address-label').text('Alamat Korespondensi');

				if (<%= profile_length != 0 %>) {
					$('#registration_type').val(registration_type);
					$('#name').val('<%= name %>');
					$('#phone').val('<%= phone %>');
					$('#datepicker').val('<%= birth_date %>');
					$('#address').val('<%= address %>');
					$('#name').attr('placeholder', 'Nama Lengkap');
					$('#phone-input').attr('class', 'form-holder');
					$('#phone').attr('placeholder', 'Nomor Handphone');
					$('#gender-input').show();
					if ('<%= gender %>' == 'male') {
						$('#gender-male').attr('selected', 'selected');
					}
					else {
						$('#gender-female').attr('selected', 'selected');
					}
					$('#established-place-company-phone-row').hide();
					$('#datepicker').attr('placeholder', 'Tanggal Lahir');
					$.when(
						$.ajax({
							type: 'get',
							url: '/complete-profile/get-city',
							data: {
								'province_id': <%= province_id %>
							},
							success: function(data, status) {
								if(data.success == true) {     
									for (let index = 0; index < data.city.length; index++) {
										if (data.city[index].id == <%= city_id %>) {
											$('#city').append('<option class="city-option" value="'+data.city[index].id+'" selected>'+data.city[index].nama+'</option>');
										}
										else {
											$('#city').append('<option class="city-option" value="'+data.city[index].id+'">'+data.city[index].nama+'</option>');
										}
									}
								}
							}
						}),
	
						$.ajax({
							type: "GET",
							url: '/complete-profile/get-district',
							data: {
								'city_id': <%= city_id %>
							},
							success: function(data, status) {
								if(data.success == true) {
									for (let index = 0; index < data.district.length; index++) {
										if (data.district[index].id == <%= district_id %>) {
											$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'" selected>'+data.district[index].nama+'</option>');
										}
										else {
											$('#district').append('<option class="ditrict-option" value="'+data.district[index].id+'">'+data.district[index].nama+'</option>');
										}
									}
								}
							}
						}),
						$.ajax({
							type: "GET",
							url: '/complete-profile/get-sub_district',
							data: {
								'district_id': <%= district_id %>
							},
							success: function(data, status) {
								if(data.success == true) {
									for (let index = 0; index < data.sub_district.length; index++) {
										if (data.sub_district[index].id == <%= sub_district_id %>) {
											$('#sub-district').append('<option class="sub_ditrict-option" value="'+data.sub_district[index].id+'" selected>'+data.sub_district[index].nama+'</option>');
										}
										else {
											$('#sub-district').append('<option class="sub_ditrict-option" value="'+data.sub_district[index].id+'">'+data.sub_district[index].nama+'</option>');
										}
									}
								}
							}
						})
					).then(function() {
					});
				}
			} else {
				$('#name-label').text('Nama Perusahaan');
				$('#name').attr('placeholder', 'Nama Perusahaan');
				$('#phone-input').attr('class', 'form-holder form-holder-2');
				$('#phone-label').text('Nomor Handphone Penanggungjawab');
				$('#phone').attr('placeholder', 'Nomor Handphone Penanggungjawab');
				$('#address-label').text('Alamat Perusahaan');
				$('#gender-input').hide();
				$('#gender-input').val('');
				$('#established-place-company-phone-row').show();
				$('#datepicker-label').attr('placeholder', 'Tanggal Berdiri');
				$('#datepicker').attr('placeholder', 'Tanggal Berdiri');
				$('#province-name').val(province_name);
				$('#city-name').val(city_name);
				$('#district-name').val(district_name);
				$('#sub-district-name').val(sub_district_name);
			}
		});

		$('#province').on('change', function (event) {
			event.preventDefault();
			var province_id = $(this).val();
			var province_name = $('#province option:selected').text();
			$('#province-name').val(province_name);
			$.ajax({
				type: "GET",
				url: '/complete-profile/get-city',
				data: {
					'province_id': province_id
				},
				success: function (data, status) {
					if (data.success == true) {
						$('.city-option').remove();
						$('.ditrict-option').remove();
						$('.sub_ditrict-option').remove();

						$('#city-placeholder').removeAttr('disabled');
						$('#district-placeholder').removeAttr('disabled');
						$('#sub-district-placeholder').removeAttr('disabled');

						$('#city-placeholder').attr('selected');
						$('#district-placeholder').attr('selected');
						$('#sub-district-placeholder').attr('selected');

						$('#city-placeholder').attr('disabled');
						$('#district-placeholder').attr('disabled');
						$('#sub-district-placeholder').attr('disabled');
						for (let index = 0; index < data.city.length; index++) {
							$('#city').append('<option class="city-option" value="' + data.city[index].id + '">' + data.city[index].nama +
								'</option>');
						}
					}
				}
			})
		});

		$('#city').on('change', function (event) {
			event.preventDefault();
			var city_id = $(this).val();
			var city_name = $('#city option:selected').text();
			$('#city-name').val(city_name);
			$.ajax({
				type: "GET",
				url: '/complete-profile/get-district',
				data: {
					'city_id': city_id
				},
				success: function (data, status) {
					if (data.success == true) {
						$('.ditrict-option').remove();
						$('.sub_ditrict-option').remove();

						$('#district-placeholder').removeAttr('disabled');
						$('#sub-district-placeholder').removeAttr('disabled');

						$('#district-placeholder').attr('selected');
						$('#sub-district-placeholder').attr('selected');

						$('#district-placeholder').attr('disabled');
						$('#sub-district-placeholder').attr('disabled');
						for (let index = 0; index < data.district.length; index++) {
							$('#district').append('<option class="ditrict-option" value="' + data.district[index].id + '">' + data.district[
								index].nama + '</option>');
						}
					}
				}
			})
		});

		$('#district').on('change', function (event) {
			event.preventDefault();
			var district_id = $(this).val();
			var district_name = $('#district option:selected').text();
			$('#district-name').val(district_name);
			$.ajax({
				type: "GET",
				url: '/complete-profile/get-sub_district',
				data: {
					'district_id': district_id
				},
				success: function (data, status) {
					if (data.success == true) {
						$('.sub_ditrict-option').remove();

						$('#sub-district-placeholder').removeAttr('disabled');

						$('#sub-district-placeholder').attr('selected');

						$('#sub-district-placeholder').attr('disabled');
						for (let index = 0; index < data.sub_district.length; index++) {
							$('#sub-district').append('<option class="sub_ditrict-option" value="' + data.sub_district[index].id + '">' +
								data.sub_district[index].nama + '</option>');
						}
					}
				}
			})
		});

		$('#sub-district').on('change', function (event) {
			event.preventDefault();
			var sub_district_name = $('#sub-district option:selected').text();
			$('#sub-district-name').val(sub_district_name);
		});
	</script>
</body>

</html>