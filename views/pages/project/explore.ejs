                                <span class="pull-right">
                                    <% let goal = 0 %>
                                    <% project.budget.forEach((budget) => { %>
                                        <% goal = goal + budget.amount %>
                                    <% }) %>
                                    Target Dana: Rp. <%= goal %>
                                </span>
                            </div>
                        </div>
                    </div>
                <% }) %>
