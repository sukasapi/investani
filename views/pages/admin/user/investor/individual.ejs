<!doctype html>
<html class="no-js" lang="">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Ela Admin - HTML5 Admin Template</title>
    <meta name="description" content="Ela Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="apple-touch-icon" href="https://i.imgur.com/QRAUqs9.png">
    <link rel="shortcut icon" href="https://i.imgur.com/QRAUqs9.png">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.0/normalize.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.2.0/css/flag-icon.min.css">
    <link rel="stylesheet" href="/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="/css/lib/datatable/dataTables.bootstrap.min.css">
    <link rel="stylesheet" href="/css/dashboard-style.css">

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>

    <!-- <script type="text/javascript" src="https://cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.min.js"></script> -->
    <style>
        #bootstrap-data-table-1_filter, #bootstrap-data-table-1_paginate, #bootstrap-data-table-2_filter, #bootstrap-data-table-2_paginate, #bootstrap-data-table-3_filter, #bootstrap-data-table-3_paginate{
            text-align: right
        }
        #bootstrap-data-table-1_filter label, #bootstrap-data-table-2_filter label, #bootstrap-data-table-3_filter label{
            text-align: left;
        }
        .pagination {
            display: -webkit-inline-box;
        }
        .detail-link, .detail-link:hover, .detail-link:focus {
            color: #fff;
        }
    </style>
</head>

<body>
    <!-- Left Panel -->

    <% include ../../../../partials/admin/aside-dashboard %>

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

         <!-- Header-->
         <% include ../../../../partials/admin/header-dashboard %>
         <!-- Header-->
 
         <% include ../../../../partials/admin/breadcrumbs-dashboard %>

        <div class="content">
            <div class="animated fadeIn">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Investor terverifikasi</strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table-1" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Nama Lengkap</th>
                                            <th>Email</th>
                                            <th>Alamat Korespondendsi</th>
                                            <th>No Handphone</th>
                                            <th>Nomor Rekening</th>
                                            <th>Nomor NPWP</th>
                                            <th>Detail</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i<users.length; i++) { %>
                                            <tr>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].profile[0].name%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].email %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].profile[0].address %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].profile[0].phone %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].bank.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].bank[0].account_number%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].document.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <%= users[i].document[0].npwp_number%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].document.length != 0 && users[i].user_type[0].status == 'verified') { %>
                                                        <div class="input-group-btn">
                                                            <a class="detail-link" href="/admin/user/investor/individual/<%= users[i]._id %>" target="_blank"><button class="btn btn-primary">Lihat Detail</button></a>
                                                        </div>
                                                    <%}%>
                                                </td>
                                            </tr>
                                        <% }%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Investor Menunggu Verifikasi</strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table-2" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Nama Lengkap</th>
                                            <th>Email</th>
                                            <th>Alamat Korespondendsi</th>
                                            <th>No Handphone</th>
                                            <th>Nomor Rekening</th>
                                            <th>Nomor NPWP</th>
                                            <th>Detail</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i<users.length; i++) { %>
                                        <tr>
                                            <td>
                                                <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].profile[0].name%>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].email %>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].profile[0].address %>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].profile.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].profile[0].phone %>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].bank.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].bank[0].account_number%>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].document.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <%= users[i].document[0].npwp_number%>
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if (users[i].document.length != 0 && users[i].user_type[0].status == 'waiting') { %>
                                                    <div class="input-group-btn">
                                                        <a class="detail-link" href="/admin/user/investor/individual/<%= users[i]._id %>" target="_blank"><button class="btn btn-primary">Lihat Detail</button></a>
                                                    </div>
                                                <%}%>
                                            </td>
                                        </tr>
                                        <% }%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Investor Aktif</strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table-3" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Nama Lengkap</th>
                                            <th>Email</th>
                                            <th>Alamat Korespondendsi</th>
                                            <th>No Handphone</th>
                                            <th>Nomor Rekening</th>
                                            <th>Nomor NPWP</th>
                                            <th>Detail</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i<users.length; i++) { %>
                                            <tr>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].profile[0].name%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].email %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].profile[0].address %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].profile.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].profile[0].phone %>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].bank.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].bank[0].account_number%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].document.length != 0 && users[i].bank.length == 0) { %>
                                                        <%= users[i].document[0].npwp_number%>
                                                    <%}%>
                                                </td>
                                                <td>
                                                    <%if (users[i].document.length != 0 && users[i].bank.length == 0) { %>
                                                        <div class="input-group-btn">
                                                            <button class="btn btn-primary">
                                                                <a href="">Lihat Detail</a>
                                                            </button>
                                                        </div>
                                                    <%}%>
                                                </td>
                                            </tr>
                                        <% }%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>

            </div><!-- .animated -->
        </div><!-- .content -->


        <div class="clearfix"></div>

        <% include ../../../../partials/admin/footer-dashboard %>

    </div><!-- /#right-panel -->

    <!-- Right Panel -->

    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>
    <script src="/js/individual-main.js"></script>

    <script src="/js/datatables.min.js"></script>
    <script src="/js/dataTables.bootstrap.min.js"></script>
    <script src="/js/dataTables.buttons.min.js"></script>
    <script src="/js/buttons.bootstrap.min.js"></script>
    <script src="/js/jszip.min.js"></script>
    <script src="/js/vfs_fonts.js"></script>
    <script src="/js/buttons.html5.min.js"></script>
    <script src="/js/buttons.print.min.js"></script>
    <script src="/js/buttons.colVis.min.js"></script>
    <script src="/js/datatables-init.js"></script>
</body>

</html>