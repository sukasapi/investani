    <script>
        <% if (user.profile[0].registration_type == "individual") { %>
            var url = '/admin/user/investor/individual/verify/<%= user._id %>'
        <% } %>
        <% if (user.profile[0].registration_type == "company") { %>
            var url = '/admin/user/investor/company/verify/<%= user._id %>'
        <% } %>
        $('#verification-button').on('click', function (event) {
            $('#verification-button').attr('disabled', 'disabled');
            $('#verification-button-process').show();
            $('#verification-button-text').hide();
            $.ajax({
                type: 'post',
                url: url,
                success: function (data, status) {
                    if (data.success == true) {
                        $('#verification-button-process').hide();
                        $('#verification-button-text').show();
                        $('#verification-button-text').text('Terverifikasi');
                        $('#success-alert').show();
                        $('#success-alert').append(data.message +
                            '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>'
                            );
                    } else {
                        $('#verification-button-process').hide();
                        $('#verification-button-text').show();
                        $('#error-alert').show();
                        $('#error-alert').append(data.message +
                            '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>'
                            );
                    }
                }
            })
        });
    </script>
