                                <div id="error-alert" class="sufee-alert alert with-close alert-danger alert-dismissible fade show" style="display: none">
                                    <span id="error-message"></span>
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div id="success-alert" class="sufee-alert alert with-close alert-success alert-dismissible fade show" style="display: none">
                                    <span id="success-message"></span>
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="tab-content" id="pills-tabContent">
                                    <div class="tab-pane fade show active" id="pills-home" role="tabpanel"
                                        aria-labelledby="pills-home-tab">
                                        <form action="/inisiator/profile/<%= user._id %>" method="post">
                                            <% if (error_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-danger alert-dismissible fade show">
                                                    <%= error_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <% if (success_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-success alert-dismissible fade show">
                                                    <%= success_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <div class="row form-group">
                                                <div class="col col-md-6">
                                                    <label class=" form-control-label">Nama Lengkap</label>
                                                    <input class="form-control" name="name" type="text" value="<%= user.profile[0].name %>">
                                                </div>
                                                <div class="col col-md-6">
                                                    <label class=" form-control-label">Email</label>
                                                    <input class="form-control" name="email" type="email" value="<%= user.email %>">
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <div class="col col-md-6">
                                                    <label for="vat" class=" form-control-label">Nomor Handphone</label>
                                                    <input class="form-control" name="phone" pattern="^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\s\./0-9]*$" type="text" value="<%= user.profile[0].phone %>">
                                                </div>
                                                <div class="col col-md-6">
                                                    <label for="gender" class=" form-control-label">Jenis Kelamin</label>
                                                    <select name="gender" class="form-control">
                                                        <option value="male" <% if (user.profile[0].gender == 'male') { %> selected <% } %>>Pria</option>
                                                        <option value="female" <% if (user.profile[0].gender == 'female') {%> selected <% } %>>Wanita</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Tanggal Lahir</label>
                                                <input class="form-control" id="datepicker" name="birth_date" type="text" value="<%= birth_date %>">
                                            </div>
                                            <div class="row form-group">
                                                <div class="col col-md-6">
                                                    <label class=" form-control-label">Provinsi</label>
                                                    <select class="form-control" id="province" name="province">
                                                        <% for(var i = 0; i < province.length; i++) {%>
                                                            <% if (province[i].id == user.profile[0].province[0].province_id) { %>
                                                                <option value="<%= province[i].id %>" selected><%= province[i].nama %></option>
                                                            <% } else { %>
                                                                <option value="<%= province[i].id %>"><%= province[i].nama %></option>
                                                            <% }%>
                                                        <% } %>
                                                    </select>
                                                    <input class="form-control" id="province-name" name="province_name" style="display: none" type="text" value="<%= user.profile[0].province[0].province_name %>">
                                                </div>
                                                <div class="col col-md-6">
                                                    <label for="city" class=" form-control-label">Kota</label>
                                                    <select class="form-control" id="city" name="city">
                                                        <option id="city-placeholder" value="" disabled selected>Pilih kota</option>
                                                    </select>
                                                    <input class="form-control" id="city-name" name="city_name" style="display: none" type="text" value="<%= user.profile[0].city[0].city_name %>">
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <div class="col col-md-6">
                                                    <label for="vat" class=" form-control-label">Kecamatan</label>
                                                    <select class="form-control" id="district" name="district">
                                                        <option id="district-placeholder" value="" disabled selected>Pilih kecamatan</option>
                                                    </select>
                                                    <input class="form-control" id="district-name" name="district_name" style="display: none" type="text" value="<%= user.profile[0].district[0].district_name %>">
                                                </div>
                                                <div class="col col-md-6">
                                                    <label for="city" class=" form-control-label">Kelurahan</label>
                                                    <select class="form-control" id="sub-district" name="sub_district">
                                                        <option id="sub-district-placeholder" value="" disabled selected>Pilih desa</option>
                                                    </select>
                                                    <input class="form-control" id="sub-district-name" name="sub_district_name" style="display: none" type="text" value="<%= user.profile[0].sub_district[0].sub_district_name %>">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Alamat Korespondensi</label>
                                                <input class="form-control" name="address" type="text" value="<%= user.profile[0].address %>">
                                            </div>
                                            <button class="btn btn-lg btn-warning btn-block" type="submit">
                                                <span id="verification-button-text">Simpan</span>
                                            </button>
                                        </form>
                                    </div>
                                    <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab">
                                        <form action="/inisiator/profile/<%= user._id %>/occupation" method="post">
                                            <% if (error_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-danger alert-dismissible fade show">
                                                    <%= error_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <% if (success_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-success alert-dismissible fade show">
                                                    <%= success_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <div class="form-group">
                                                <label class=" form-control-label">Pekerjaan</label>
                                                <select name="occupation" class="form-control">
                                                    <option disabled selected>Pilih pekerjaan</option>
                                                    <option value="Karyawan swasta" <% if (user.occupation[0].occupation == "Karyawan swasta") { %> selected <% } %>>Karyawan swasta</option>
                                                    <option value="Pegawai negeri sipil" <% if (user.occupation[0].occupation == "Pegawai negeri sipil") { %> selected <% } %>>Pegawai negeri sipil</option>
                                                    <option value="Pengusaha" <% if (user.occupation[0].occupation == "Pengusaha") { %> selected <% } %>>Pengusaha</option>
                                                    <option value="TNI / Polisi" <% if (user.occupation[0].occupation == "TNI / Polisi") { %> selected <% } %>>TNI / Polisi</option>
                                                    <option value="Karyawan BUMN" <% if (user.occupation[0].occupation == "Karyawan BUMN") { %> selected <% } %>>Karyawan BUMN</option>
                                                    <option value="Pensiun" <% if (user.occupation[0].occupation == "Pensiun") { %> selected <% } %>>Pensiun</option>
                                                    <option value="Ibu/Bapak rumah tangga" <% if (user.occupation[0].occupation == "Ibu/Bapak rumah tangga") { %> selected <% } %>>Ibu/Bapak rumah tangga</option>
                                                    <option value="Pelajar" <% if (user.occupation[0].occupation == "Pelajar") { %> selected <% } %>>Pelajar</option>
                                                    <option value="Profesional" <% if (user.occupation[0].occupation == "Profesional") { %> selected <% } %>>Profesional</option>
                                                    <option value="Lainnya" <% if (user.occupation[0].occupation == "Lainnya") { %> selected <% } %>>Lainnya</option>
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Nama Perusahaan (Opsional)</label>
                                                <input class="form-control" name="company_name" type="text" value="<%= user.occupation[0].company_name %>">
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Alamat Perusahaan (Opsional)</label>
                                                <input class="form-control" name="company_address" type="text" value="<%= user.occupation[0].company_address %>">
                                            </div>
                                            <div class="row form-group">
                                                <div class="col col-md-6">
                                                    <label for="vat" class=" form-control-label">Jabatan (Opsional)</label>
                                                    <select name="position" class="form-control">
                                                        <option selected disabled>Pilih jabatan</option>
                                                        <option value="komisaris" <% if (user.occupation[0].position == 'komisaris' ) { %> selected <% } %>>Komisaris (setara non-eksekutif)</option>
                                                        <option value="eksekutif_perusahaan" <% if (user.occupation[0].position == 'eksekutif_perusahaan' ) { %> selected <% } %>>Eksekutif Perusahaan (setara direksi)</option>
                                                        <option value="manajer" <% if (user.occupation[0].position == 'manajer' ) { %> selected <% } %>>Manajer (atau setara)</option>
                                                        <option value="kepala_bagian_supervisor" <% if (user.occupation[0].position == 'kepala_bagian_supervisor' ) { %> selected <% } %>>Kepala Bagian/Supervisor</option>
                                                        <option value="karyawan_staff" <% if (user.occupation[0].position == 'karyawan_staff' ) { %> selected <% } %>>Karyawan/Staff</option>
                                                        <option value="lainnya" <% if (user.occupation[0].position == 'lainnya' ) { %> selected <% } %>>Lainnya</option>
                                                    </select>
                                                </div>
                                                <div class="col col-md-6">
                                                    <label for="vat" class=" form-control-label">Sumber Dana</label>
                                                    <select name="income_source" class="form-control">
                                                        <option disabled selected>Pilih sumber dana</option>
                                                        <option value="penghasilan_rutin_gaji" <% if (user.occupation[0].income_source == "penghasilan_rutin_gaji") { %> selected <% } %>>Penghasilan rutin/Gaji</option>
                                                        <option value="usaha_bisnis" <% if (user.occupation[0].income_source == "usaha_bisnis") { %> selected <% } %>>Usaha/Bisnis</option>
                                                        <option value="hasil_investasi" <% if (user.occupation[0].income_source == "hasil_investasi") { %> selected <% } %>>Hasil Investani</option>
                                                        <option value="warisan" <% if (user.occupation[0].income_source == "warisan") { %> selected <% } %>>Warisan</option>
                                                        <option value="hadiah" <% if (user.occupation[0].income_source == "hadiah") { %> selected <% } %>>Hadiah</option>
                                                        <option value="lainnya" <% if (user.occupation[0].income_source == "lainnya") { %> selected <% } %>>Lainnya</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Penghasilan per Bulan</label>
                                                <select name="income" id="income" class="form-control">
                                                    <option disabled selected>Pilih penghasilan</option>
                                                    <option value="5 juta" <% if (user.occupation[0].income == "5 juta") { %> selected <% } %>>s/d 5 juta</option>
                                                    <option value="5 - 10 juta" <% if (user.occupation[0].income == "5 - 10 juta") { %> selected <% } %>>5 - 10 juta</option>
                                                    <option value="10 - 20 juta" <% if (user.occupation[0].income == "10 - 20 juta") { %> selected <% } %>>10 - 20 juta</option>
                                                    <option value="20 - 50 juta" <% if (user.occupation[0].income == "20 - 50 juta") { %> selected <% } %>>20 - 50 juta</option>
                                                    <option value="di atas 50 juta" <% if (user.occupation[0].income == "di atas 50 juta") { %> selected <% } %>>di atas 50 juta</option>
                                                </select>
                                            </div>
                                            <button class="btn btn-lg btn-warning btn-block" type="submit">
                                                <span id="verification-button-text">Simpan</span>
                                            </button>
                                        </form>
                                    </div>
                                    <div class="tab-pane fade" id="pills-document" role="tabpanel" aria-labelledby="pills-document-tab">
                                        <form action="/inisiator/profile/<%= user._id %>/document" enctype="multipart/form-data" method="post">
                                            <% if (error_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-danger alert-dismissible fade show">
                                                    <%= error_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <% if (success_message != '') { %>
                                                <div class="sufee-alert alert with-close alert-success alert-dismissible fade show">
                                                    <%= success_message %>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            <% } %>
                                            <div class="form-group">
                                                <label class=" form-control-label">NPWP (Opsional)</label>
                                                <input class="form-control" name="npwp_number" type="number" placeholder="Nomor NPWP" value="<%= user.document[0].npwp_number %>">
                                            </div>
                                            <div class="form-group">
                                                <label class=" form-control-label">Unggah NPWP (Opsional)</label>
                                                <div class="input-group">
                                                    <input accept="image/*" class="form-control-file" name="npwp_image" type="file">
                                                    <div class="input-group-btn">
                                                        <button type="button" class="btn btn-primary mb-1"
                                                            data-toggle="modal" data-target="#npwp-image-modal">
                                                            Lihat
                                                        </button>
                                                    </div>
                                                </div>
                                                <input type="text" name="npwp_image_input" value="<%= user.document[0].npwp_image %>" style="display: none">
                                                <label for="exampleFormControlFile1" id="npwp-image-filename-label"><%= user.document[0].npwp_image %></label>
                                            </div>
                                            <button class="btn btn-lg btn-warning btn-block" type="submit">
                                                <span id="verification-button-text">Simpan</span>
                                            </button>
                                        </form>
                                    </div>
                                    <div class="tab-pane fade" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab">
